<div class="modal fade" id="<%= 'menu-modal-' + index %>" tabindex="-1" role="dialog"
     aria-labelledby="center-title" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="center-title"><%= item.name %></h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <img src="<%= item.imgPath %>" alt="" class="img-fluid ">
                <h5 class="text-center modal-price"><%= item.price %></h5>
                <p class=""><%- item.des %></p>
            </div>
            <div class="modal-footer">
                <!--<button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>-->
                <!--<button type="button" class="btn btn-primary">Save changes</button>-->
            </div>
        </div>
    </div>
</div>